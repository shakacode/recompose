{
  "name": "recompose-build",
  "private": true,
  "author": "Andrew Clark <acdlite@me.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/acdlite/recompose.git"
  },
  "license": "MIT",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "lint": "eslint scripts src",
    "build:recompose": "cross-env PACKAGE_NAME=recompose rollup --config scripts/rollup.config.js",
    "test": "jest && flow check && cross-env SNAPSHOT=match npm run build:recompose",
    "test:watch": "cross-env BABEL_ENV=cjs jest --watch",
    "release": "node scripts/release.js",
    "postinstall": "node scripts/installNestedPackageDeps.js",
    "format": "prettier --write 'scripts/*.js' 'src/packages/*/*.js' 'src/packages/*/!(node_modules)/**/*.js'",
    "precommit": "lint-staged",
    "prepush": "yarn test"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*-test.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "testTimeout": 20000,
    "testEnvironment": "jsdom",
    "restoreMocks": true
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.22.0",
    "@babel/core": "^7.22.0",
    "@babel/plugin-transform-class-properties": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/runtime": "^7.22.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^29.7.0",
    "baconjs": "^0.7.84",
    "chalk": "^1.1.1",
    "change-case": "^2.3.1",
    "codecov": "^1.0.1",
    "create-react-class": "^15.5.0",
    "cross-env": "^4.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.72.0",
    "flyd": "^0.2.4",
    "husky": "^0.13.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "kefir": "^3.2.3",
    "lint-staged": "^3.4.0",
    "most": "^1.0.2",
    "prettier": "^1.19.1",
    "prop-types": "^15.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "readline-sync": "^1.2.21",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^4.0.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rx": "^4.1.0",
    "rxjs": "^5.0.0",
    "shelljs": "^0.6.0",
    "xstream": "^5.0.5"
  },
  "dependencies": {}
}
